{
    "name": "sepia",
    "displayName": "Sepia Theme",
    "description": "Your eyes will thank you",
    "version": "1.0.0",
    "publisher": "ZYinMD",
    "repository": "https://github.com/ZYinMD/sepia",
    "icon": "icon.png",
    "engines": {
        "vscode": "^1.29.0"
    },
    "categories": [
        "Themes"
    ],
    "keywords": [
        "parchment",
        "vintage",
        "solarized",
        "yellow",
        "brown"
    ],
    "contributes": {
        "themes": [
            {
                "label": "Sepia",
                "uiTheme": "vs",
                "path": "./themes/sepia.json"
            },
            {
                "label": "Sepia from Cuttlefish",
                "uiTheme": "vs-dark",
                "path": "./themes/sepia-from-cuttlefish.json"
            }
        ]
    },
    "scripts": {
        "visualize": "node ./scripts/visualize.js",
        "postvisualize": "printf 'For faster speed, you might want to run `node ./scripts/visualize.js` directly'",
        "postversion": "vsce pubilsh",
        "vscode:prepublish": "auto-changelog -p && git add CHANGELOG.md && git commit --amend --no-edit"
    },
    "devDependencies": {
        "auto-changelog": "^1.10.2",
        "comment-json": "^1.1.3"
    },
    "dependencies": {
        "cssjson": "^2.1.3",
        "json-css": "^1.5.6"
    }
}
