	"tokenColors": [
		{
			"name": "default color",
			"scope": [
				"text",
				"source"
			],
			"settings": {
				"foreground": "#8d796b"
			}
		},
		{
			"name": "data structures, types and identifiers, e.g. function names",
			"scope": [
				"entity.name"
			],
			"settings": {
				"foreground": "#094"
			}
		},
		{
			"name": "function calls",
			"scope": [
				"meta.function-call",
				"meta.method-call",
				"meta.function-call entity.name.function",
				"meta.method-call entity.name.function"
			],
			"settings": {
				"foreground": "#009b98"
			}
		},
		{
			"name": "inherited class",
			"scope": [
				"entity.other.inherited-class"
			],
			"settings": {
				"foreground": "#b96900d7"
			}
		},
		{
			"name": "variables",
			"scope": [
				"variable"
			],
			"settings": {
				"foreground": "#879900"
			}
		},
		{
			"name": "properties",
			"scope": [
				"variable.other.property",
				"variable.other.object.property",
				"support.type.property-name",
				"meta.object-literal.key"
			],
			"settings": {
				"foreground": "#879900"
			}
		},
		{
			"name": "reserved variables of the language: self, this, super, etc",
			"scope": [
				"variable.language"
			],
			"settings": {
				"foreground": "#094"
			}
		},
		{
			"name": "built-in functions and variables of runtime: require, window, document, process, etc",
			"scope": [
				"support.variable",
				"support.class",
				"support.constant",
				"support.function"
			],
			"settings": {
				"foreground": "#088"
			}
		},
		{
			"name": "function parameter declaration",
			"scope": [
				"variable.parameter",
				"entity.name.variable.parameter"
			],
			"settings": {
				"foreground": "#67b"
			}
		},
		{
			"name": "variable.function",
			"scope": [
				"variable.function"
			],
			"settings": {
				"foreground": "#088"
			}
		},
		{
			"name": "booleans, null, undefined, escape, symbols, etc",
			"scope": [
				"constant"
			],
			"settings": {
				"foreground": "#aa00aabb"
			}
		},
		{
			"name": "numbers",
			"scope": [
				"constant.numeric"
			],
			"settings": {
				"foreground": "#f57b00"
			}
		},
		{
			"name": "var, const, let, int, bool, char, func, function, class, def",
			"scope": [
				"storage.type",
				"keyword.type",
				"punctuation.definition.annotation"
			],
			"settings": {
				"foreground": "#038dbeee"
			}
		},
		{
			"name": "static, inline, const, public, private",
			"scope": [
				"storage.modifier"
			],
			"settings": {
				"foreground": "#088"
			}
		},
		{
			"name": "legal but unsure / user constructs",
			"scope": [
				"support.type"
			],
			"settings": {
				"foreground": "#b96900d7"
			}
		},
		{
			"name": "import / export",
			"scope": [
				"keyword.import",
				"keyword.export",
				"keyword.control.import",
				"keyword.control.export",
				"keyword.other.import",
				"keyword.other.export",
				"keyword.other.using",
				"keyword.control.directive.include"
			],
			"settings": {
				"foreground": "#c10c"
			}
		},
		{
			"name": "flow control",
			"scope": [
				"keyword.control"
			],
			"settings": {
				"foreground": "#c10c"
			}
		},
		{
			"name": "conditional flow control",
			"scope": [
				"keyword.control.conditional",
				"keyword.control.trycatch",
				"keyword.control.try",
				"keyword.control.loop",
				"switch-expression.expr keyword.control.switch"
			],
			"settings": {
				"foreground": "#222"
			}
		},
		{
			"name": "operators",
			"scope": [
				"keyword.operator"
			],
			"settings": {
				"foreground": "#f57b00"
			}
		},
		{
			"name": "other keyword",
			"scope": [
				"keyword"
			],
			"settings": {
				"foreground": "#038dbeee"
			}
		},
		{
			"name": "string",
			"scope": [
				"string",
				"punctuation.definition.string"
			],
			"settings": {
				"foreground": "#a7882d"
			}
		},
		{
			"name": "Regular Expressions",
			"scope": [
				"string.regexp",
				"string.regexp punctuation.definition.string.begin",
				"string.regexp punctuation.definition.string.end",
				"constant.other.character-class.set.regexp",
				"constant.other.character-class.regexp",
				"constant.character.escape.backslash.regexp"
			],
			"settings": {
				"foreground": "#e44d"
			}
		},
		{
			"name": "variable interpolation",
			"scope": [
				"meta.string-contents.quoted.double punctuation.definition.variable",
				"punctuation.definition.interpolation",
				"punctuation.definition.template-expression.begin",
				"punctuation.definition.template-expression.end",
				"punctuation.definition.variable.makefile"
			],
			"settings": {
				"foreground": "#e44d"
			}
		},
		{
			"name": "comment",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"foreground": "#999d"
			}
		},
		{
			"name": "invalid",
			"scope": [
				"invalid"
			],
			"settings": {
				"foreground": "#f00"
			}
		},
		{
			"name": "invalid.deprecated",
			"scope": [
				"invalid.deprecated"
			],
			"settings": {
				"foreground": "#f00d86cc"
			}
		},
		{
			"name": "directive",
			"scope": [
				"punctuation.definition.directive"
			],
			"settings": {
				"foreground": "#c10c"
			}
		},
		{
			"name": "c#: variable assignment",
			"scope": [
				"source.cs entity.name.variable"
			],
			"settings": {
				"foreground": "#839400"
			}
		},
		{
			"name": "c#: function name",
			"scope": [
				"entity.name.function.cs"
			],
			"settings": {
				"foreground": "#094"
			}
		},
		{
			"name": "c#: new",
			"scope": [
				"keyword.other.new.cs"
			],
			"settings": {
				"foreground": "#c10c"
			}
		},
		{
			"name": "c: default color",
			"scope": [
				"source.c",
				"source.cpp"
			],
			"settings": {
				"foreground": "#879900"
			}
		},
		{
			"name": "c: punctuation color reset",
			"scope": [
				"source.c punctuation",
				"source.cpp punctuation"
			],
			"settings": {
				"foreground": "#8d796b"
			}
		},
		{
			"name": "c: parameters",
			"scope": [
				"meta.function.c"
			],
			"settings": {
				"foreground": "#67b"
			}
		},
		{
			"name": "c: arguments",
			"scope": [
				"meta.function-call.c"
			],
			"settings": {
				"foreground": "#879900"
			}
		},
		{
			"name": "CSS: property names",
			"scope": [
				"support.type.property-name",
				"support.type.vendored.property-name"
			],
			"settings": {
				"foreground": "#879900"
			}
		},
		{
			"name": "CSS: property value",
			"scope": [
				"meta.property-value",
				"support.constant.property-value",
				"support.constant.font-name",
				"support.constant.media-type",
				"support.constant.media"
			],
			"settings": {
				"foreground": "#a7882d"
			}
		},
