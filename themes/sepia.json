	"tokenColors": [
		{
			"scope": "token.info-token",
			"settings": {
				"foreground": "#316bcd"
			}
		},
		{
			"scope": "token.warn-token",
			"settings": {
				"foreground": "#cd9731"
			}
		},
		{
			"scope": "token.error-token",
			"settings": {
				"foreground": "#cd3131"
			}
		},
		{
			"scope": "token.debug-token",
			"settings": {
				"foreground": "#800080"
			}
		},
		{
			"name": "default color",
			"scope": [
				"text",
				"source",
				"punctuation"
			],
			"settings": {
				"foreground": "#8d796b"
			}
		},
		{
			"name": "data structures, types and identifiers, e.g. function names",
			"scope": [
				"entity.name"
			],
			"settings": {
				"foreground": "#094e"
			}
		},
		{
			"name": "function calls",
			"scope": [
				"meta.function-call",
				"meta.method-call",
				"meta.function-call entity.name.function",
				"meta.method-call entity.name.function"
			],
			"settings": {
				"foreground": "#30a195"
			}
		},
		{
			"name": "inherited class",
			"scope": [
				"entity.other.inherited-class"
			],
			"settings": {
				"foreground": "#088"
			}
		},
		{
			"name": "variables",
			"scope": [
				"variable"
			],
			"settings": {
				"foreground": "#890"
			}
		},
		{
			"name": "properties",
			"scope": [
				"variable.other.property",
				"variable.other.object.property",
				"support.type.property-name",
				"meta.object-literal.key"
			],
			"settings": {
				"foreground": "#890"
			}
		},
		{
			"name": "reserved variables of the language: self, this, super, etc",
			"scope": [
				"variable.language"
			],
			"settings": {
				"foreground": "#088"
			}
		},
		{
			"name": "built-in functions and variables of runtime: require, window, document, process, etc",
			"scope": [
				"support.variable",
				"support.class",
				"support.constant",
				"support.function"
			],
			"settings": {
				"foreground": "#088"
			}
		},
		{
			"name": "function parameter declaration",
			"scope": [
				"variable.parameter"
			],
			"settings": {
				"foreground": "#67a"
			}
		},
		{
			"name": "variable.function",
			"scope": [
				"variable.function"
			],
			"settings": {
				"foreground": "#088"
			}
		},
		{
			"name": "booleans, null, undefined, escape, symbols, etc",
			"scope": [
				"constant"
			],
			"settings": {
				"foreground": "#909a"
			}
		},
		{
			"name": "numbers",
			"scope": [
				"constant.numeric"
			],
			"settings": {
				"foreground": "#f77b00e7"
			}
		},
		{
			"name": "var, const, let, int, bool, char, func, function, class, def",
			"scope": [
				"storage.type"
			],
			"settings": {
				"foreground": "#08bd"
			}
		},
		{
			"name": "static, inline, const, public, private",
			"scope": [
				"storage.modifier"
			],
			"settings": {
				"foreground": "#088"
			}
		},
		{
			"name": "legal but unsure / user constructs",
			"scope": [
				"support.type"
			],
			"settings": {
				"foreground": "#67a"
			}
		},
		{
			"name": "import / export",
			"scope": [
				"keyword.import",
				"keyword.export",
				"keyword.control.import",
				"keyword.control.export",
				"keyword.other.import",
				"keyword.other.export",
				"keyword.other.using",
				"keyword.control.directive.include"
			],
			"settings": {
				"foreground": "#c10c"
			}
		},
		{
			"name": "flow control",
			"scope": [
				"keyword.control"
			],
			"settings": {
				"foreground": "#c10c"
			}
		},
		{
			"name": "conditional flow control",
			"scope": [
				"keyword.control.conditional",
				"keyword.control.trycatch",
				"keyword.control.try",
				"keyword.control.loop",
				"switch-expression.expr keyword.control.switch"
			],
			"settings": {
				"foreground": "#2d2d2d"
			}
		},
		{
			"name": "operators",
			"scope": [
				"keyword.operator"
			],
			"settings": {
				"foreground": "#f77b00e7"
			}
		},
		{
			"name": "other keyword",
			"scope": [
				"keyword"
			],
			"settings": {
				"foreground": "#08bd"
			}
		},
		{
			"name": "string",
			"scope": [
				"string",
				"punctuation.definition.string"
			],
			"settings": {
				"foreground": "#b79533"
			}
		},
		{
			"name": "Regular Expressions",
			"scope": [
				"string.regexp",
				"string.regexp punctuation.definition.string.begin",
				"string.regexp punctuation.definition.string.end",
				"constant.other.character-class.set.regexp",
				"constant.other.character-class.regexp",
				"constant.character.escape.backslash.regexp"
			],
			"settings": {
				"foreground": "#e53d"
			}
		},
		{
			"name": "variable interpolation",
			"scope": [
				"meta.string-contents.quoted.double punctuation.definition.variable",
				"punctuation.definition.interpolation",
				"punctuation.definition.template-expression.begin",
				"punctuation.definition.template-expression.end",
				"punctuation.definition.variable.makefile"
			],
			"settings": {
				"foreground": "#f77b00e7"
			}
		},
		{
			"name": "comment",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"foreground": "#999c"
			}
		},
		{
			"name": "invalid",
			"scope": [
				"invalid"
			],
			"settings": {
				"foreground": "#f00"
			}
		},
		{
			"name": "invalid.deprecated",
			"scope": [
				"invalid.deprecated"
			],
			"settings": {
				"foreground": "#f08c"
			}
		},
		{
			"name": "directive",
			"scope": [
				"punctuation.definition.directive"
			],
			"settings": {
				"foreground": "#c10c"
			}
		},
		{
			"name": "c: default color",
			"scope": [
				"source.c",
				"source.cpp"
			],
			"settings": {
				"foreground": "#890"
			}
		},
		{
			"name": "c: parameters",
			"scope": [
				"meta.function.c"
			],
			"settings": {
				"foreground": "#67a"
			}
		},
		{
			"name": "c: arguments",
			"scope": [
				"meta.function-call.c"
			],
			"settings": {
				"foreground": "#890"
			}
		},
		{
			"name": "CSS: property names",
			"scope": [
				"support.type.property-name",
				"support.type.vendored.property-name"
			],
			"settings": {
				"foreground": "#890"
			}
		},
		{
			"name": "CSS: property value",
			"scope": [
				"meta.property-value",
				"support.constant.property-value",
				"support.constant.font-name",
				"support.constant.media-type",
				"support.constant.media"
			],
			"settings": {
				"foreground": "#b79533"
			}
		},
		{
			"name": "CSS: @",
			"scope": [
				"keyword.control.at-rule",
				"keyword.control.at-rule punctuation.definition.keyword"
			],
			"settings": {
				"foreground": "#094e"
			}
		},
		{
			"name": "CSS: import",
			"scope": [
				"keyword.control.at-rule.import",
				"keyword.control.at-rule.import punctuation.definition.keyword"
			],
			"settings": {
				"foreground": "#c10c"
			}
		},
		{
			"name": "less and sass: variable",
			"scope": [
				"variable.scss",
				"variable.other.less",
				"variable punctuation.definition"
			],
			"settings": {
				"foreground": "#088"
			}
		},
		{
			"name": "CSS: !important",
			"scope": "keyword.other.important",
			"settings": {
				"foreground": "#c10c"
			}
		},
		{
			"name": "color values",
			"scope": [
				"constant.other.color.rgb-value",
				"constant.other.rgb-value",
				"support.constant.color",
				"keyword.other.unit.px"
			],
			"settings": {
				"foreground": "#f77b00e7"
			}
		},
		{
			"name": "color names in English",
			"scope": [
				"support.constant.color.w3c-standard-color-name.css",
				"support.constant.color.w3c-extended-color-name"
			],
			"settings": {
				"foreground": "#8d796b"
			}
		},
		{
			"name": "restore puntuation color, and some edge cases",
			"scope": [
