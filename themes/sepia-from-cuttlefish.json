  "tokenColors": [
    {
      "name": "default color",
      "scope": ["text", "source"],
      "settings": {
        "foreground": "#b0b0b0"
      }
    },
    {
      "name": "data structures, types and identifiers, e.g. function names",
      "scope": ["entity.name"],
      "settings": {
        "foreground": "#3c0"
      }
    },
    {
      "name": "function calls",
      "scope": [
        "meta.function-call",
        "meta.method-call",
        "meta.function-call entity.name.function",
        "meta.method-call entity.name.function"
      ],
      "settings": {
        "foreground": "#9c3f"
      }
    },
    {
      "name": "inherited class",
      "scope": ["entity.other.inherited-class"],
      "settings": {
        "foreground": "#e99"
      }
    },
    {
      "name": "variables",
      "scope": ["variable"],
      "settings": {
        "foreground": "#3ea67ce7"
      }
    },
    {
      "name": "properties",
      "scope": [
        "variable.other.property",
        "variable.other.object.property",
        "support.type.property-name",
        "meta.object-literal.key"
      ],
      "settings": {
        "foreground": "#3ea67ce7"
      }
    },
    {
      "name": "reserved variables of the language: self, this, super, etc",
      "scope": ["variable.language"],
      "settings": {
        "foreground": "#da7"
      }
    },
    {
      "name": "built-in functions and variables of runtime: require, window, document, process, etc",
      "scope": [
        "support.variable",
        "support.class",
        "support.constant",
        "support.function"
      ],
      "settings": {
        "foreground": "#8dce"
      }
    },
    {
      "name": "function parameter declaration",
      "scope": ["variable.parameter", "entity.name.variable.parameter"],
      "settings": {
        "foreground": "#ed8e"
      }
    },
    {
      "name": "variable.function",
      "scope": ["variable.function"],
      "settings": {
        "foreground": "#8dce"
      }
    },
    {
      "name": "booleans, null, undefined, escape, symbols, etc",
      "scope": ["constant"],
      "settings": {
        "foreground": "#a9e"
      }
    },
    {
      "name": "numbers",
      "scope": ["constant.numeric"],
      "settings": {
        "foreground": "#e4a300"
      }
    },
    {
      "name": "var, const, let, int, bool, char, func, function, class, def",
      "scope": [
        "storage.type",
        "keyword.type",
        "punctuation.definition.annotation"
      ],
      "settings": {
        "foreground": "#5cee"
      }
    },
    {
      "name": "static, inline, const, public, private",
      "scope": ["storage.modifier"],
      "settings": {
        "foreground": "#5bbd"
      }
    },
    {
      "name": "legal but unsure / user constructs",
      "scope": ["support.type"],
      "settings": {
        "foreground": "#da7"
      }
    },
    {
      "name": "import / export",
      "scope": [
        "keyword.import",
        "keyword.export",
        "keyword.control.import",
        "keyword.control.export",
        "keyword.other.import",
        "keyword.other.export",
        "keyword.other.using",
        "keyword.control.directive.include"
      ],
      "settings": {
        "foreground": "#e03a67ec"
      }
    },
    {
      "name": "flow control",
      "scope": ["keyword.control"],
      "settings": {
        "foreground": "#e03a67ec"
      }
    },
    {
      "name": "conditional flow control",
      "scope": [
        "keyword.control.conditional",
        "keyword.control.trycatch",
        "keyword.control.try",
        "keyword.control.loop",
        "switch-expression.expr keyword.control.switch"
      ],
      "settings": {
        "foreground": "#fffd"
      }
    },
    {
      "name": "operators",
      "scope": ["keyword.operator"],
      "settings": {
        "foreground": "#e03a67ec"
      }
    },
    {
      "name": "other keyword",
      "scope": ["keyword"],
      "settings": {
        "foreground": "#5cee"
      }
    },
    {
      "name": "string",
      "scope": ["string", "punctuation.definition.string"],
      "settings": {
        "foreground": "#dd0c"
      }
    },
    {
      "name": "Regular Expressions",
      "scope": [
        "string.regexp",
        "string.regexp punctuation.definition.string.begin",
        "string.regexp punctuation.definition.string.end",
        "constant.other.character-class.set.regexp",
        "constant.other.character-class.regexp",
        "constant.character.escape.backslash.regexp"
      ],
      "settings": {
        "foreground": "#e99"
      }
    },
    {
      "name": "variable interpolation",
      "scope": [
        "meta.string-contents.quoted.double punctuation.definition.variable",
        "punctuation.definition.interpolation",
        "punctuation.definition.template-expression.begin",
        "punctuation.definition.template-expression.end",
        "punctuation.definition.variable.makefile"
      ],
      "settings": {
        "foreground": "#e03a67ec"
      }
    },
    {
      "name": "comment",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#899b"
      }
    },
    {
      "name": "invalid",
      "scope": ["invalid"],
      "settings": {
        "foreground": "#f64e"
      }
    },
    {
      "name": "invalid.deprecated",
      "scope": ["invalid.deprecated"],
      "settings": {
        "foreground": "#ff698c"
      }
    },
    {
      "name": "directive",
      "scope": ["punctuation.definition.directive"],
      "settings": {
        "foreground": "#e03a67ec"
      }
    },
    {
      "name": "c#: variable assignment",
      "scope": ["source.cs entity.name.variable"],
      "settings": {
        "foreground": "#0a7e"
      }
    },
    {
      "name": "c#: function name",
      "scope": ["entity.name.function.cs"],
      "settings": {
        "foreground": "#9c3f"
      }
    },
    {
      "name": "c#: new",
      "scope": ["keyword.other.new.cs"],
      "settings": {
        "foreground": "#e03a67ec"
      }
    },
    {
      "name": "c: default color",
      "scope": ["source.c", "source.cpp"],
      "settings": {
        "foreground": "#3ea67ce7"
      }
    },
    {
      "name": "c: punctuation color reset",
      "scope": ["source.c punctuation", "source.cpp punctuation"],
      "settings": {
        "foreground": "#b0b0b0"
      }
    },
    {
      "name": "c: parameters",
      "scope": ["meta.function.c"],
      "settings": {
        "foreground": "#ed8e"
      }
    },
    {
      "name": "c: arguments",
      "scope": ["meta.function-call.c"],
      "settings": {
        "foreground": "#3ea67ce7"
      }
    },
    {
      "name": "CSS: property names",
      "scope": [
        "support.type.property-name",
        "support.type.vendored.property-name"
      ],
      "settings": {
        "foreground": "#9c3f"
      }
    },
    {
      "name": "CSS: property value",
      "scope": [
        "meta.property-value",
        "support.constant.property-value",
        "support.constant.font-name",
        "support.constant.media-type",
        "support.constant.media"
      ],
      "settings": {
        "foreground": "#dd0c"
      }
    },
    {
      "name": "CSS: @",
      "scope": [
        "keyword.control.at-rule",
        "keyword.control.at-rule punctuation.definition.keyword"
      ],
      "settings": {
        "foreground": "#e03a67ec"
      }
    },
    {
      "name": "CSS: import",
      "scope": [
        "keyword.control.at-rule.import",
        "keyword.control.at-rule.import punctuation.definition.keyword"
      ],
      "settings": {
        "foreground": "#e03a67ec"
      }
    },
    {
      "name": "less and sass: variable",
      "scope": [
        "variable.scss",
        "variable.other.less",
        "variable punctuation.definition"
      ],
      "settings": {
        "foreground": "#0a7e"
      }
    },
    {
      "name": "CSS: !important",
      "scope": "keyword.other.important",
      "settings": {
        "foreground": "#e03a67ec"
      }
    },
    {
      "name": "CSS: color values and sizing units",
      "scope": [
        "constant.other.color.rgb-value",
        "constant.other.rgb-value",
        "support.constant.color",
        "keyword.other.unit"
      ],
      "settings": {
        "foreground": "#e4a300"
      }
    },
    {
      "name": "SCSS: ()[]{} (seen in functions inside styled-components in react",
      "scope": ["variable.interpolation.scss"],
      "settings": {
        "foreground": "#b0b0b0"
      }
    },
    {
      "name": "color names in English",
      "scope": [
        "support.constant.color.w3c-standard-color-name.css",
        "support.constant.color.w3c-extended-color-name"
      ],
      "settings": {
        "foreground": "#b0b0b0"
      }
    },
    {
      "name": "restore puntuation color, and some edge cases",
      "scope": [
        "constant.other.symbol.hashkey.ruby",
        "keyword.operator.dereference.java",
        "keyword.operator.navigation.groovy",
        "meta.scope.for-loop.shell punctuation.definition.string.begin",
        "meta.scope.for-loop.shell punctuation.definition.string.end",
        "meta.scope.for-loop.shell string",
        "punctuation.separator.list.comma.css",
        "constant.language.empty-list.haskell",
        "punctuation.section.embedded",
        "punctuation.definition.block",
        "punctuation.terminator"
      ],
      "settings": {
        "foreground": "#b0b0b0"
      }
    },
    {
      "name": "go: default",
      "scope": ["source.go"],
